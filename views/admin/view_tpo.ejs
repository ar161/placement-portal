<!-- view_tpo.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View TPOs</title>
</head>
<body>
    <h1>List of TPOs</h1>
    <ul>
        <% tpos.forEach(tpo => { %>
            <li><%= tpo.name %> - <%= tpo.email %></li>
        <% }); %>
    </ul>
</body>
</html>
